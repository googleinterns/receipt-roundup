{
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "moment": true,
    "noUiSlider": true,
    "google": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "no-multi-spaces": "off",
    "no-unused-vars": ["error", { "vars": "local" }],
    "require-jsdoc": "off",
    "valid-jsdoc": "off"
  }
}
